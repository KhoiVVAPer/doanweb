@model QLHD.Models.Course

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/index.cshtml";
}

<h2>THÔNG TIN CHI TIẾT</h2>

<div class="Details">
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã khóa học
        </dt>

        <dd>
            @Html.DisplayFor(model => model.idCourse)
        </dd>
        <dt>
            Năm bắt đầu
        </dt>

        <dd>
            @Html.DisplayFor(model => model.YearStart)
        </dd>

        <dt>
            Năm kết thúc
        </dt>

        <dd>
            @Html.DisplayFor(model => model.YearEnd)
        </dd>

    </dl>
</div>
<div class="EditBack">
    <a class="Edit" href="@Url.Action("Edit", "AdminKhoaHoc",new { idCourse = Model.idCourse })"><span class="glyphicon glyphicon-pencil"></span><p style="font-size:15px"> ĐIỀU CHỈNH</p></a> |
    <a class="Back" href="@Url.Action("Index","AdminKhoaHoc")"><span class="glyphicon glyphicon-circle-arrow-left"></span><p style="font-size:15px"> TRỞ VỀ</p></a>
</div>
